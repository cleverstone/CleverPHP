{
  "name": "cleverstone/cleverphp",
  "description": "The Cleverphp Framework. ",
  "keywords": [
    "framework",
    "cleverphp",
    "cop"
  ],
  "homepage": "http://cleverstone.cn/",
  "license": "MIT",
  "authors": [
    {
      "name": "cleverstone",
      "email": "yang_hui_lei@163.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/log": "~1.0",
    "cleverstone/cleverphp-src": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "app\\": "app",
      "ext\\": "ext"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "clever\\tests\\": "tests"
    }
  },
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}